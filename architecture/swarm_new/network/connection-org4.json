{
    "name": "first-network-org4",
    "version": "1.0.0",
    "client": {
        "organization": "Org4",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "Org4": {
            "mspid": "Org4MSP",
            "peers": [
                "peer0.org4.example.com",
                "peer1.org4.example.com"
            ],
            "certificateAuthorities": [
                "ca.org4.example.com"
            ]
        }
    },
    "peers": {
        "peer0.org4.example.com": {
            "url": "grpcs://$ORG4_IP:13051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/org4.example.com/tlsca/tlsca.org4.example.com-cert.pem"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.org4.example.com"
            }
        },
        "peer1.org4.example.com": {
            "url": "grpcs://$ORG4_IP:14051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/org4.example.com/tlsca/tlsca.org4.example.com-cert.pem"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer1.org4.example.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.org4.example.com": {
            "url": "https://$ORG4_IP:10054",
            "caName": "ca-org4",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/org4.example.com/tlsca/tlsca.org4.example.com-cert.pem"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
